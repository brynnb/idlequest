{
  "name": "idlequest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "test:inventory": "npx playwright test e2e/inventory-persistence.spec.ts --headed",
    "server:go": "cd server-go && (air || go run ./cmd/daemon)",
    "server": "set -a && source .env && set +a && npm run server:go",
    "dev:all": "npm run server:go & sleep 3 && npm run dev"
  },
  "dependencies": {
    "@tanstack/react-query-devtools": "^5.58.0",
    "capnp-es": "^0.0.11",
    "dotenv": "^16.4.5",
    "mysql2": "^3.12.0",
    "openai": "^4.67.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-router-dom": "^6.26.2",
    "socket.io-client": "^4.7.2",
    "sql.js": "^1.11.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.13",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.50.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/sql.js": "^1.4.9",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jsdom": "^25.0.0",
    "redux-devtools-extension": "^2.13.9",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.8"
  }
}