{
    "name": "idlequest-server",
    "version": "1.0.0",
    "description": "Combat server for IdleQuest",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "start:safe": "node start-server.js",
        "dev": "ts-node --esm src/index.ts",
        "test": "jest",
